generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model WeatherData {
  id          Int      @id @default(autoincrement())
  temperature Float
  humidity    Float
  light       Float
  pressure    Float
  date        DateTime @default(now())
}
